<p id="notice"><%= notice %></p>

<p>
  <b>Project:</b>
  <%= @issue.project.name rescue "-" %>
</p>

<p>
  <b>Subject:</b>
  <%= @issue.subject %>
</p>

<p>
  <b>Opener user:</b>
  <%= @issue.opener_user.email rescue "-" %>
</p>

<p>
  <b>Assignee user:</b>
  <%= @issue.assignee_user.email rescue "-" %>
</p>

<p>
  <b>Status:</b>
  <%= @issue.status.name rescue "-" %>
</p>


<%= link_to 'Edit', edit_issue_path(@issue) %> |
<%= link_to 'Back', issues_path %>
