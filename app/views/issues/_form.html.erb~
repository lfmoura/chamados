<%= simple_form_for(@issue) do |f| %>
  <%= f.error_notification %>

  <div class="inputs">
    <%= f.association :project, :collection => Project.ativos  Project.ativos.to_sql%>
    <%= f.input :subject %>
    <%= f.association :opener_user %>
    <%= f.association :assignee_user %>
    <%= f.association :status %>
  </div>

  <div class="actions">
    <%= f.button :submit %>
  </div>
<% end %>
